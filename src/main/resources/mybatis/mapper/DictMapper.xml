<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.springbootdemo.mapper.DictMapper">
    <select id="getAllDictData" resultType="com.demo.springbootdemo.pojo.DictData">
        select type, name, value, label
        from dict
        order by type, value
    </select>

</mapper>